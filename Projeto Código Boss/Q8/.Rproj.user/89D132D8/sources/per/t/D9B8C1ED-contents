#Dados#
n <- 6
sumxi <- 20.2
sumxisqr <- 95.36
sumyi <- 550
sumyisqr <- 61386
sumxiyi <- 1322.2

alpha <- 0.05
x0 <- 3.7



#Contas#
B1 <- (sumxiyi - sumxi*sumyi/n)/(sumxisqr - sumxi*sumxi/n)
B0 <- sumyi/n - B1*sumxi/n

sigma_sqr <- ((sumyisqr - sumyi*sumyi/n) - B1*B1*(sumxisqr - sumxi*sumxi/n))/(n-2)
comboio <- sqrt((1/n + (sumxi/n - x0)*(sumxi/n - x0)/(sumxisqr - sumxi*sumxi/n))*sigma_sqr)

Y_0 = B0 + B1*x0
Ymin <-Y_0-qt(1-alpha/2, n-2)*comboio
Ymax <-Y_0+qt(1-alpha/2, n-2)*comboio 
Ymin
Ymax