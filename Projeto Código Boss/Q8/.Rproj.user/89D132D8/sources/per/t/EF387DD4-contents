#Dados#
miu <- 55
desvpdr <- 4
Oi <- c(11, 50, 122, 67)

n <- sum(Oi)
Ei_aux <- vector(length = 4)


#Contas#
Ei_aux[1] <- pnorm(50, mean = miu, sd = desvpdr)
Ei_aux[2] <- pnorm(55,miu,desvpdr) - pnorm(50,miu,desvpdr)
Ei_aux[3] <- pnorm(60,miu,desvpdr) - pnorm(55,miu,desvpdr)
Ei_aux[4] <- 1 - pnorm(60,miu,desvpdr)

Ei = n*Ei_aux

v <- 0
for (i in 1:4){
  v<-v+(Oi[i]-Ei[i])*(Oi[i]-Ei[i])/Ei[i]
}

1-pchisq(v, 3)

