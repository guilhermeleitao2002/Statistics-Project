#DADOS#

n <- 6
tempo <- c(2,4,7,8, 9, 11)
log_residuo <- c(0.59, 0.41, 0.37, 0.35, 0.32,0.31)



sumxi <- 41
sumxisqr <- 335
sumyi <- 2.35
sumyisqr <- 0.9741
sumxiyi <- 14.5




#CONTAS#
B1 <- (sumxiyi - sumxi*sumyi/n)/(sumxisqr - sumxi*sumxi/n)

sigma_sqr <- ((sumyisqr - sumyi*sumyi/n) - B1*B1*(sumxisqr - sumxi*sumxi/n))/(n-2)
comboio <- sqrt(sigma_sqr/(sumxisqr - sumxi*sumxi/n))

v = B1/comboio

valor_p <- 2*(1-pt(abs(v),n-2))
valor_p