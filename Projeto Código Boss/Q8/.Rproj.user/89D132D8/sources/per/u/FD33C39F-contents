#Dados#
lambda <- 0.5
title <- c(0,1,2,3,4)
Oi <- c(66, 63, 35, 38, 48)
n <- sum(Oi)
Ei_aux <- vector(length = n)


#Contas#
for (i in 1:4) 
{
  Ei_aux[i] <- (title[i] + 1)*(1 - lambda)^(title[i])*lambda*lambda
}

Ei_aux[5] = 1 - (Ei_aux[1] + Ei_aux[2] + Ei_aux[3] + Ei_aux[4])

Ei = n*Ei_aux



v <- 0
for (i in 1:5){
  v<-v+(Oi[i]-Ei[i])*(Oi[i]-Ei[i])/Ei[i]
}

1-pchisq(v, 4)